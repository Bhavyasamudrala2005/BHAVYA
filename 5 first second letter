#include<stdio.h>                                                               
int main()   
{     
	double a,b, product;
    printf("enter the first letter:");
	scanf("%lf",&a);                                                                                              
	printf("enter the second letter:");
	scanf("%lf",&b);
	product =a*b;
	printf("product=%.2lf",product);                   
	return 0;                                                                                                   
}                            
